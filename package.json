{
  "name": "react-webpack-app",
  "version": "1.0.0",
  "description": "A biolerplate to spinup a webpack based react app.",
  "main": "src/app.js",
  "scripts": {
    "webpack": "webpack",
    "start": "webpack-dev-server --config ./webpack.config.js --content-base app --host 127.0.0.1 --port 8080 --colors",
    "build": "webpack --config ./webpack.config.js --progress --profile --colors",
    "prod": "NODE_ENV=production node server.js",
    "postinstall": "webpack --config ./webpack.config.js --progress --profile --colors",
    "lint": "./node_modules/.bin/eslint src",
    "lint-fixup": "./node_modules/.bin/eslint src --ext .js,.jsx --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/visha12l/React-boiler-plate.git"
  },
  "keywords": [
    "react",
    "webpack",
    "biolerplate"
  ],
  "author": "Vishal Shinde",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/visha12l/React-boiler-plate/issues"
  },
  "homepage": "https://github.com/visha12l/React-boiler-plate#readme",
  "dependencies": {
    "babel-core": "^6.24.1",
    "babel-eslint": "^8.2.3",
    "babel-loader": "^7.0.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^0.28.0",
    "form-serialize": "^0.7.2",
    "html-webpack-plugin": "^2.28.0",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.1.1",
    "react-select": "^1.0.0-rc.5",
    "react-validation": "^2.10.9",
    "style-loader": "^0.16.1",
    "underscore": "^1.12.1",
    "validator": "^8.2.0",
    "webpack": "^2.4.1",
    "webpack-dev-server": "^2.4.5"
  },
  "devDependencies": {
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-babel": "^5.1.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.8.2"
  }
}
